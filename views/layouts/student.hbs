<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/public/lib/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/public/lib/font/css/font-awesome.min.css">
    <link rel="stylesheet" href="/public/css/student.css">
    <title>Personal</title>
</head>

<body data-spy="scroll" data-target=".jump-nav" data-offset="20">
    {{{body}}}
    <script type="text/javascript" src="/public/lib/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.bootcss.com/popper.js/1.14.7/umd/popper.min.js"></script>
    <script type="text/javascript" src="/public/lib/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/public/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script>
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $('.btn-jump').on('click', function (event) {
                $('.jump-group>li').removeClass('active');
                $(this).parent().parent().addClass('active');
            })
            $(window).scroll(function () {
                var w_top = $(window).scrollTop();
                if (w_top >= 0 && w_top <= 544) {
                    $('.jump-group>li').removeClass('active');
                    $('#l1').addClass('active')
                } else if (w_top <= 1062) {
                    $('.jump-group>li').removeClass('active');
                    $('#l2').addClass('active')
                } else if (w_top <= 2146) {
                    $('.jump-group>li').removeClass('active');
                    $('#l3').addClass('active')
                } else if (w_top <= 2600) {
                    $('.jump-group>li').removeClass('active');
                    $('#l4').addClass('active')
                } else {
                    $('.jump-group>li').removeClass('active');
                    $('#l5').addClass('active')
                }
            })
        });

    </script>
</body>

</html>